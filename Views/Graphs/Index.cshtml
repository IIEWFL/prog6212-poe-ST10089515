@model IEnumerable<WebModuleApp.Models.Graph>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HoursPerWeek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdealHoursPerWeek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Module.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semester.SemesterId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudyRecord.StudyRecordId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HoursPerWeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdealHoursPerWeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Module.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semester.SemesterId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudyRecord.StudyRecordId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.GraphId }) |
            @Html.ActionLink("Details", "Details", new { id=item.GraphId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.GraphId })
        </td>
    </tr>
}

</table>
