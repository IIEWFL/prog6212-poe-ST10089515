@model IEnumerable<WebModuleApp.Models.Semester>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumberofModules)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberofWeeks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumberofModules)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberofWeeks)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SemesterId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SemesterId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SemesterId })
        </td>
    </tr>
}

</table>
